{
    "extends": "airbnb",
    "valid-jsdoc": ["error", {
      "preferType": {
        "String": "string",
        "object": "Object"
        }
      }],
    "require-jsdoc": ["error", {
        "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true
        }
    }],
    "plugins": [
        "react"
    ]
}
